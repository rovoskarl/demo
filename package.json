{
  "name": "smart_helper",
  "version": "1.0.1",
  "private": true,
  "scripts": {
    "android": "ENVFILE=.env  react-native run-android --mode=developmentDebug --appId=com.tastien.smart_helper.test",
    "android:prod": "ENVFILE=.env.prod react-native run-android --mode=productionDebug --appId=com.tastien.smart_helper",
    "android:stage": "ENVFILE=.env.stage react-native run-android --mode=stageDebug --appId=com.tastien.smart_helper.stage",
    "build:android": "fastlane android build_test",
    "build:android:prod": "bundle exec fastlane android build_prod",
    "build:android:stage": "bundle exec fastlane android build_stage",
    "clean": "cd android && ./gradlew clean",
    "ios": "react-native run-ios --scheme 'dev'",
    "lint": "eslint .",
    "pod-install": "cd ios && RCT_NEW_ARCH_ENABLED=1 bundle exec pod install",
    "prepare": "husky install",
    "start": "react-native start --reset-cache",
    "start:debug": "react-native start --experimental-debugger"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --report-unused-disable-directives --max-warnings 10",
      "prettier --write"
    ],
    "*.json": [
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  },
